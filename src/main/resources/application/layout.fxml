<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" minHeight="300.0" minWidth="450.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem fx:id="save" mnemonicParsing="false" text="Save current values" />
            <MenuItem fx:id="load" mnemonicParsing="false" text="Load default values" />
            <SeparatorMenuItem fx:id="quit" mnemonicParsing="false" />
            <MenuItem fx:id="quit" mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="130.0" minWidth="130.0" prefWidth="130.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="310.20001220703125" minWidth="10.0" prefWidth="291.20002441406245" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="BX" GridPane.halignment="CENTER" />
                  <Label text="CX1" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
                  <Label text="CX2" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
                  <Label text="EventService" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
                  <Label text="Vcenter1" GridPane.halignment="CENTER" GridPane.rowIndex="4" />
                  <Label text="Vcenter2" GridPane.halignment="CENTER" GridPane.rowIndex="5" />
                  <ChoiceBox fx:id="bx" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
                  <ChoiceBox fx:id="cx1" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
                  <ChoiceBox fx:id="cx2" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
                  <ChoiceBox fx:id="es" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
                  <ChoiceBox fx:id="vc1" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="4" />
                  <ChoiceBox fx:id="vc2" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="5" />
                  <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="6">
                     <children>
                        <TextField fx:id="path" HBox.hgrow="ALWAYS" />
                        <Button fx:id="browse" mnemonicParsing="false" text="Browse..." />
                     </children>
                  </HBox>
                  <Label text="Files location" GridPane.halignment="CENTER" GridPane.rowIndex="6" />
               </children>
            </GridPane>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
    </AnchorPane>
      <Separator prefWidth="200.0" />
      <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" spacing="10.0">
         <children>
            <Button fx:id="update" defaultButton="true" mnemonicParsing="false" text="Update files" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
  </children>
</VBox>
